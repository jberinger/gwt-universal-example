<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://www.gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to="gwtutest">
    <inherits name="com.google.gwt.user.User" />
    <inherits name="elemental2.core.Core" />
    <inherits name="elemental2.dom.Dom" />
    <inherits name="net.sprd.gwt.GwtUniversal" />
    

    <entry-point class="net.sprd.gwt.client.GwtUniversalEntryPoint" />

    <set-property name="user.agent" value="safari,gecko1_8" />
    <set-property-fallback name="user.agent" value="safari"/>
    
    <set-configuration-property name="user.agent.runtimeWarning" value="false"/>

    <set-configuration-property name="devModeRedirectEnabled" value="true"/>
    
    <add-linker name="xsiframe"/>
    

    <source path="client" />
    <source path="shared" />
</module>
